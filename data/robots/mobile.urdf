  <!--==================== Physical Links =================================-->

  <link name="linkBase">

    <inertial>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
      <mass value="0.5" />
      <inertia
        ixx="8.33e-3"
        ixy="1.66e-3"
        ixz="6.67e-3"
        iyy="5.42e-3"
        iyz="3.75e-3"
        izz="1.04e-2" />
    </inertial>

    <visual>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
      <geometry>
        <box size="0.3 0.4 0.2" />
      </geometry>
      <material name="teal"/>
    </visual>

    <visual>
      <origin xyz="-0.25 0.05 -0.12" rpy="0 -2 0" />
      <geometry>
        <box size="0.05 0.05 0.4" />
      </geometry>
    </visual>

    <visual>
      <origin xyz="-0.25 -0.05 -0.12" rpy="0 -2 0" />
      <geometry>
        <box size="0.05 0.05 0.4" />
      </geometry>
    </visual>

    <collision>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
      <geometry>
        <box size="0.3 0.4 0.2" />
      </geometry>
    </collision>

  </link>


  <link name="linkWheelLeft">

    <inertial>
      <origin xyz="0.0 0.0 0.025" rpy="0 0 0" />
      <mass value="0.1" />
      <inertia
        ixx="2.27e-3"
        ixy="0.0"
        ixz="0.0"
        iyy="2.27e-3"
        iyz="0.0"
        izz="4.5e-3" />
    </inertial>
    
    <visual>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
      <geometry>
        <cylinder radius="0.3" length="0.05"/>
      </geometry>
      <material name="black"/>
    </visual>

    <visual>
      <origin xyz="0.0 0.0 -0.05" rpy="0 0 0" />
      <geometry>
        <cylinder radius="0.03" length="0.1"/>
      </geometry>
    </visual>

    <contact>
      <friction_anchor/>
      <stiffness value="30000.0"/>
      <damping value="1000.0"/>
      <spinning_friction value="0.1"/>
      <lateral_friction value="1.0"/>
    </contact>

    <collision>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
      <geometry>
        <cylinder radius="0.3" length="0.05"/>
      </geometry>
    </collision>

  </link>


  <link name="linkWheelRight">

    <inertial>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
      <mass value="0.1" />
      <inertia
        ixx="2.27e-3"
        ixy="0.0"
        ixz="0.0"
        iyy="2.27e-3"
        iyz="0.0"
        izz="4.5e-3" />
    </inertial>
    
    <visual>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
      <geometry>
        <cylinder radius="0.3" length="0.05"/>
      </geometry>
      <material name="black"/>
    </visual>

    <visual>
      <origin xyz="0.0 0.0 0.05" rpy="0 0 0" />
      <geometry>
        <cylinder radius="0.03" length="0.1"/>
      </geometry>
    </visual>

    <contact>
      <friction_anchor/>
      <stiffness value="30000.0"/>
      <damping value="1000.0"/>
      <spinning_friction value="0.1"/>
      <lateral_friction value="1.0"/>
    </contact>

    <collision>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
      <geometry>
        <cylinder radius="0.3" length="0.05"/>
      </geometry>
    </collision>

  </link>


  <link name="linkBallCaster">

    <inertial>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
      <mass value="0.3" />
      <inertia
        ixx="2.57e-4"
        ixy="0.0"
        ixz="0.0"
        iyy="2.57e-4"
        iyz="0.0"
        izz="5.0e-4" />
    </inertial>
    
    <visual>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
      <geometry>
        <cylinder radius="0.1" length="0.03"/>
      </geometry>
      <material name="maroon"/>
    </visual>

    <visual>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
      <geometry>
        <cylinder radius="0.01" length="0.1"/>
      </geometry>
    </visual>

    <collision>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
      <geometry>
        <cylinder radius="0.1" length="0.03"/>
      </geometry>
    </collision>

  </link>


  <!--==================== Active Joints ==================================-->

  <joint name="jointActiveLeft" type="continuous">

    <origin xyz="0.0 -0.3 0.0" rpy="1.57079632679 0  0" />
    <parent link="linkBase" />
    <child link="linkWheelLeft" />
    <axis xyz="0 0 1" />
    <limit effort="500" velocity="500"/>
    <joint_properties damping=".0" friction=".0"/>

  </joint>


  <joint name="jointActiveRight" type="continuous">

    <origin xyz="0.0 0.3 0.0" rpy="1.57079632679 0 0" />
    <parent link="linkBase" />
    <child link="linkWheelRight" />
    <axis xyz="0 0 1" />
    <limit effort="500" velocity="500"/>
    <joint_properties damping=".0" friction=".0"/>

  </joint>


  <joint name="jointFixed" type="continuous">

    <origin xyz="-0.4 0.0 -0.2" rpy="1.57079632679 0 0" />
    <parent link="linkBase" />
    <child link="linkBallCaster" />
    <axis xyz="0 0 1" />
    <limit effort="500" velocity="500"/>
    <joint_properties damping=".0" friction=".0"/>

  </joint>

